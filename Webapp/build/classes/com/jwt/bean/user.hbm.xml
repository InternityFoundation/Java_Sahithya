<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
<hibernate-mapping>
    <class name="com.jwt.bean.UserControllerServlet" table="emp">
        <id column="ID" name="id" type="java.lang.Integer" />
        <property column="NAME" name="userName" type="java.lang.String" />
        <property column="ADDRESS" name="password1" type="string" />
        <property column="PHONE" name="phone" type="java.lang.String" />
        <property column="EMAIL" name="email" type="java.lang.String" />
        <property column="EXP" name="exp" type="java.lang.String" />
    </class>
</hibernate-mapping>